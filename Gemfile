source "https://rubygems.org"

# Jekyll plugins
gem "github-pages", group: :jekyll_plugins do
  gem "jekyll-remote-theme"
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"
  gem "jekyll-paginate"
  gem "jekyll-algolia"
end

gem 'wdm', '>= 0.1.0', platforms: [:mingw, :x64_mingw, :mswin] if Gem::Platform.local.os == 'mswin32'